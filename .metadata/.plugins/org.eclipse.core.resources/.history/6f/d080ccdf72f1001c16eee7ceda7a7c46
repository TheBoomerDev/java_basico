package test.ejercicios;

public class ej01 {

	public static void main(String[] args) {
		  
		int var1 = 15;
		int var2 = 4; 
		
		// SUMA
		int suma = var1 + var2;
		
		// RESTA
		int resta = var1 - var2;
		
		// DIVISION
		int division = var1 / var2;
		
		// MULTIPLICACIO
		int multi = var1 * var2;
		
		System.out.println(suma);
		System.out.println(resta);
		System.out.println(division);
		System.out.println(multi);
		
	}
}
